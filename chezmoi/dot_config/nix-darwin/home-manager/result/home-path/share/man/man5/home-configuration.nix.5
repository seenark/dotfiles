    path = \(dq/\(dq;
    info_type = \(dqavailable\(dq;
    interval = 60;
    warning = 20\&.0;
    alert = 10\&.0;
  }
  {
    block = \(dqsound\(dq;
    format = \(dq $icon $output_name {$volume\&.eng(w:2) |}\(dq;
    click = [
      {
        button = \(dqleft\(dq;
        cmd = \(dqpavucontrol \-\-tab=3\(dq;
