\fBprograms\&.autorandr\&.hooks\fR
.RS 4
Global hook scripts
.sp
\fIType:\fR submodule
.sp
\fIDefault:\fR { }
.sp
\fIExample:\fR
.sp
.RS 4
.nf
{
  postswitch = {
    \(dqnotify\-i3\(dq = \(dq${pkgs\&.i3}/bin/i3\-msg restart\(dq;
    \(dqchange\-background\(dq = readFile \&./change\-background\&.sh;
    \(dqchange\-dpi\(dq = \(aq\(aq
      case \(dq$AUTORANDR_CURRENT_PROFILE\(dq in
        default)
          DPI=120
          ;;
        home)
          DPI=192
          ;;
        work)
          DPI=144
          ;;
        *)
          echo \(dqUnknown profile: $AUTORANDR_CURRENT_PROFILE\(dq
          exit 1
      esac

      echo \(dqXft\&.dpi: $DPI\(dq | ${pkgs\&.xorg\&.xrdb}/bin/xrdb \-merge
    \(aq\(aq
  };
}
.fi
.RE
