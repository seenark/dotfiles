  top = {
    # The title block
    title = {
      interval = \(dqpersist\(dq;
      command = \(dqxtitle \-s\(dq;
    };
  };
  bottom = {
    time = {
      command = \(dqdate +%r\(dq;
      interval = 1;
    };
    # Make sure this block comes after the time block
    date = lib\&.hm\&.dag\&.entryAfter [ \(dqtime\(dq ] {
      command = \(dqdate +%d\(dq;
      interval = 5;
    };
    # And this block after the example block
    example = lib\&.hm\&.dag\&.entryAfter [ \(dqdate\(dq ] {
      command = \(dqecho hi $(date +%s)\(dq;
      interval = 3;
    };
  };
}
